{
  "manifest_version": 3,
  "name": "URL Safety Checker",
  "version": "1.0",
  "description": "Checks URL with backend and warns user if suspicious or malicious.",
  "permissions": ["tabs", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"],
      "css": ["warning.css"]
    }
  ]
}